{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "process",
			"label": "cargo build",
			"command": "cargo",
			"args": [
				"build"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build",
		},
		{
			"type": "process",
			"label": "cargo run",
			"command": "cargo",
			"args": [
				"run"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build",
		},
		{
			"type": "process",
			"label": "cargo watch",
			"command": "cargo",
			"args": [
				"watch"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build",
		},
		{
			"type": "process",
			"label": "cargo test",
			"command": "cargo",
			"args": [
				"test"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"group": "build",
		}
	]
}